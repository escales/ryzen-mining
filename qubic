mkdir ~/qubic;
cd ~/qubic;
wget https://dl.qubic.li/downloads/qli-Client-1.8.7.1-Linux-x64.tar.gz;
tar -xvf qli-Client-1.8.7.1-Linux-x64.tar.gz;
